
<system-context version="1.0">
		<% @data.each do |key, val| %>
        <<%=key%>><%=val%></<%=key%>>
		<% end %>
		
        <system-config>
            <% @sys_config.each do |key, val| %>
			<<%=key%>><%=val%></<%=key%>>
			<% end %>
        </system-config>
		
        <!-- determines properties of system -->
        <properties>
				<% @properties.each do |key, val| %>
                <property name="<%=key%>"><%=val%></property>
				<% end %>
        </properties>
</system-context>
